#!/bin/bash

# Colors

bold='\033[1m' # Bold Text

black='\033[30m'
white='\033[37m'
cyan='\033[36m'

red='\033[41m' # HighLight
hcyan='\033[46m' # HighLight

reset='\033[0m' # Reset Colors

## Start Android Debug Bridge

adb start-server



user=$USER # Get User of system


echo -e """$white



	██╗  ██╗██╗   ██╗ ██████╗ ███╗   ██╗██╗   ██╗███████╗     █████╗ ███╗   ██╗██████╗ ██████╗  ██████╗ ██╗██████╗ 
